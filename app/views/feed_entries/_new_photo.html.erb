<div class="feed_item photo_feed" id="<%= o.dom_id %>">
  <div class="left">
    <img src="/images/blurp_photos.png" class="png icon_small" title="User Photo Icon"  alt="User Photo Icon" /> 
    <a href="<%= image_path o.ref_obj.image %>" title="<%= o.ref_obj.caption %>" class="thickbox"><%= o.text %></a>
    <% if o.count > 1 %>
      <% o.targets[1..-1].each do |p| %>
      <a href="<%= image_path p.image %>" title="<%= p.caption %>" class="thickbox hidden">&nbsp;</a>
      <% end %>
    <% end %>
  </div>
  <div class="right">
    &nbsp;
    <%# x_feed_link(feed_item) if me %>
  </div>
</div>